{
  "name": "@vafast/rate-limit",
  "version": "0.0.1",
  "description": "Plugin for Vafast for rate limiting",
  "license": "MIT",
  "scripts": {
    "dev": "bun run --watch example/vafast-basic.ts",
    "test": "bun test",
    "build": "bun build.ts",
    "release": "npm run build && npm run test && npm publish --access public"
  },
  "peerDependencies": {
    "vafast": ">= 0.1.12"
  },
  "devDependencies": {
    "@types/bun": "1.1.14",
    "eslint": "9.6.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vafast": "^0.1.13"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "vafast",
    "rate-limit",
    "throttling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vafastjs/vafast-rate-limit"
  },
  "author": {
    "name": "saltyAom",
    "url": "https://github.com/SaltyAom",
    "email": "saltyaom@gmail.com"
  },
  "homepage": "https://github.com/vafastjs/vafast-rate-limit",
  "bugs": "https://github.com/vafastjs/vafast-rate-limit/issues",
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0"
  }
}
