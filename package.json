{
  "name": "@vafast/rate-limit",
  "version": "0.0.11",
  "description": "Plugin for Vafast for rate limiting",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsdown",
    "dev": "npx tsx watch example/index.ts",
    "test": "npx vitest run",
    "release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
  },
  "peerDependencies": {
    "vafast": "^0.5.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^24.0.0",
    "rimraf": "^6.0.1",
    "tsdown": "^0.19.0-beta.4",
    "typescript": "^5.4.5",
    "vafast": "^0.5.1",
    "vitest": "^4.0.16"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [
    "vafast",
    "rate-limit",
    "throttling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vafast/vafast-rate-limit"
  },
  "author": {
    "name": "saltyAom",
    "url": "https://github.com/SaltyAom",
    "email": "saltyaom@gmail.com"
  },
  "homepage": "https://github.com/vafast/vafast-rate-limit",
  "bugs": "https://github.com/vafast/vafast-rate-limit/issues",
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0"
  }
}
